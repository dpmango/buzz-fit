{
  "name": "bf_new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "lint:js:fix": "eslint './src/**/*.{js,jsx}' --fix",
    "deploy": "craco build && cp build/index.html build/200.html && surge --project ./build --domain buzz-fit.surge.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.18",
    "@craco/craco": "^6.1.2",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "core-js": "^3.12.1",
    "date-fns": "^2.21.3",
    "dayjs": "^1.10.4",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "mobx": "^6.3.0",
    "mobx-react": "^7.1.0",
    "mobx-utils": "^6.0.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.13.1",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.0",
    "react-slick": "^0.28.1",
    "react-slidedown": "^2.4.5",
    "react-spinners": "^0.10.6",
    "react-transition-group": "^4.4.1",
    "retry-axios": "^2.4.0",
    "viewport-units-buggyfill": "^0.6.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
